{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Solver",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/solver/__main__.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "-a",
                "dfs",
                "levels/level9.json"
            ],
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "-a",
                "dfs",
                "levels/level9.json"
            ],
            "env": {
                "DEBUG": "true"
            }
        }
    ]
}